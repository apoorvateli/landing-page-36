<h3>contact form</h3>

<form action="/createContact" method="post">

    <label for="name">NAME:</label>
    <input type="text" placeholder="Name" name="contact[name]" id="name" required>

    <label for="email">EMAIL:</label>
    <input type="text" placeholder="Email address" name="contact[email]" id="email" required>

    <label for="message">MESSAGE:</label>
    <input type="text" placeholder="Your message" name="contact[message]" id="message" required>

    <button type="submit">Submit</button>
</form>
